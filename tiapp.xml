<?xml version="1.0" encoding="UTF-8"?><ti:app xmlns:ti="http://ti.appcelerator.org">
    <id>com.ericwhitlock.poimen</id>
    <name>Poimen</name>
    <version>0.8.3</version>
    <publisher>Poimen Ministries</publisher>
    <url>http://www.enjoycreativity.com</url>
    <description>This app was created by Enjoy Creativity for Poiment Ministries.</description>
    <copyright>2013 by Enjoy Creativity</copyright>
    <icon>appicon.png</icon>
    <persistent-wifi>false</persistent-wifi>
    <prerendered-icon>false</prerendered-icon>
    <statusbar-style>default</statusbar-style>
    <statusbar-hidden>false</statusbar-hidden>
    <fullscreen>true</fullscreen>
    <navbar-hidden>true</navbar-hidden>
    <analytics>true</analytics>
    <guid>95a74700-1f77-4360-9f9f-fdeeb89f1941</guid>
    <property name="ti.ui.defaultunit" type="string">system</property>
    <iphone>
        <orientations device="iphone">
